{
  "name": "Rick Roulette",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Randomly replaces youtube videos with Rick Rolls.",
  "homepage_url": "http://www.bugben.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "src/options/options.html",
  "permissions": [
    "contentSettings",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*.youtube.com/*",
        "https://*.youtube.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
